{
  "title": "CAPS_LOCK to Hyper, Hyper + wasd navigation and others.",
  "maintainer": "andrsdt",
  "rules": [
    {
      "description": "CapsLock to Hyper",
      "manipulators": [
        {
          "description": "capslock = capslock(click) | hyper(hold)",
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_shift",
              "modifiers": ["right_command", "right_control", "right_option"]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "caps_lock"
            }
          ]
        }
      ]
    },
    {
      "description": "Hyper + SPACE : Toggle Spotlight/Alfred",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "spacebar",
              "modifiers": ["left_command"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Hyper + command + C : File start",
      "manipulators": [
        {
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option",
                "left_command"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": ["left_command"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Hyper + command + V : File end",
      "manipulators": [
        {
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option",
                "left_command"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow",
              "modifiers": ["left_command"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Hyper + C : Home",
      "manipulators": [
        {
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "home"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Hyper + V : End",
      "manipulators": [
        {
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Hyper + Backspace : Delete",
      "manipulators": [
        {
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option"
              ],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
