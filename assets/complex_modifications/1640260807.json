    {
      "description": "Hyper + command + Q/E to next/previous desktops",
      "manipulators": [
        {
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option",
                "left_command"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": ["left_control"],
              "repeat": true
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "right_command",
                "right_control",
                "right_option",
                "left_command"
              ],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["left_control"],
              "repeat": true
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "right command + N to enye",
      "manipulators": [
        {
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "key_code": "n",
              "modifiers": ["left_alt"]
            },
            {
              "key_code": "n",
              "repeat": true
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Hyper + F to mission control",
      "manipulators": [
